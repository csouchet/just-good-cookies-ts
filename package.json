{
  "name": "just-good-cookies-ts",
  "version": "0.0.1",
  "description": "A delicious cookie consent solution for complying with EU cookie law",
  "license": "MIT",
  "main": "dist/justgoodcookies.esm.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "dev": "vite",
    "all": "run-s clean lint lint-check build test",
    "clean": "rimraf build dist",
    "build": "rollup -c",
    "build-bundles": "rollup -c",
    "prepack": "run-s clean build-bundles",
    "docs": "run-s docs:*",
    "docs:api": "typedoc --tsconfig ./tsconfig.typedoc.json src/bpmn-visualization.ts",
    "lint": "eslint \"*/**/*.{js,mjs,ts}\" NOTICE --max-warnings 0 --fix",
    "lint-check": "eslint \"*/**/*.{js,mjs,ts}\" NOTICE --max-warnings 0",
    "version-prepare": "node scripts/manage-version-in-files.mjs && git commit --no-verify -a -m \"[RELEASE] Prepare version for release\"",
    "version": "IS_RELEASING=true node scripts/manage-version-in-files.mjs && git add .",
    "postversion": "node scripts/manage-version-in-files.mjs && git commit --no-verify -a -m \"[RELEASE] Prepare version for new developments\"",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "."
  },
  "bugs": {
    "url": "https://github.com/csouchet/just-good-cookies-ts/issues"
  },
  "keywords": [
    "cookie",
    "cookie law",
    "cookie consent",
    "cookie consent solution",
    "cookie banner",
    "banner",
    "eu Cookie",
    "GDPR",
    "gdpr-cookie",
    "cookies-alert",
    "typescript",
    "material-ui",
    "front-end"
  ],
  "homepage": "https://github.com/francescomugnai/just-good-cookies",
  "author": {
    "name": "CÃ©line Souchet"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-notice": "^0.9.10",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "pinst": "^3.0.0",
    "prettier": "^2.7.1",
    "rollup": "^2.77.2",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-license": "^2.7.0",
    "rollup-plugin-node-externals": "^4.1.1",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-sizes": "^1.0.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "ts-migrate": "^0.1.31",
    "typedoc": "^0.23.10",
    "typescript": "^4.7.4",
    "vite": "^3.0.4"
  },
  "lint-staged": {
    "*.{js,mjs,ts}": [
      "eslint --fix"
    ],
    "NOTICE": [
      "eslint --fix"
    ]
  }
}
